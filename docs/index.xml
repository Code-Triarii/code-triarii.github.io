      <description>fetch(&#39;https://api.github.com/orgs/code-triarii/repos&#39;).then(response = {console.log(&#34;Respuesta recibida:&#34;, response);return response.json();}).then(data = {console.log(&#34;Datos recibidos:&#34;, data);let reposHTML = &#39;Our repositories:&#39;;data.forEach(repo = {reposHTML += `${repo.name}${repo.description}
Estrellas: ${repo.stargazers_count}
`;});document.getElementById(&#39;repos-list&#39;).innerHTML = reposHTML;}).catch(error = {console.log(&#34;Error al obtener datos:&#34;, error);});</description>
      <content>
&lt;div id=&#34;repos-list&#34;&gt;&lt;/div&gt;

&lt;script&gt;
    fetch(&#39;https://api.github.com/orgs/code-triarii/repos&#39;)
    .then(response =&gt; {
        console.log(&#34;Respuesta recibida:&#34;, response);
        return response.json();
    })
    .then(data =&gt; {
        console.log(&#34;Datos recibidos:&#34;, data);
        let reposHTML = &#39;&lt;h2&gt;Our repositories:&lt;/h2&gt;&#39;;
        data.forEach(repo =&gt; {
            reposHTML += `
                &lt;div class=&#34;repo-item&#34;&gt;
                    &lt;h3&gt;&lt;a href=&#34;${repo.html_url}&#34; target=&#34;_blank&#34;&gt;${repo.name}&lt;/a&gt;&lt;/h3&gt;
                    &lt;p&gt;${repo.description}&lt;/p&gt;
                    &lt;p&gt;Estrellas: ${repo.stargazers_count}&lt;/p&gt;
                &lt;/div&gt;
            `;
        });
        document.getElementById(&#39;repos-list&#39;).innerHTML = reposHTML;
    })
    .catch(error =&gt; {
        console.log(&#34;Error al obtener datos:&#34;, error);
    });
&lt;/script&gt;

</content>
